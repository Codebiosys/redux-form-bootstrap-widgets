{
  "name": "redux-form-bootstrap-widgets",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/redux-form-bootstrap-widgets.js",
  "config": {
    "builddir": "./dist"
  },
  "jest": {
    "verbose": true,
    "clearMocks": true,
    "transform": {
      ".*": "babel-jest"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/cssStub.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "modulePaths": [
      "src"
    ],
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.jsx"
    ],
    "coverageDirectory": "./__coverage__",
    "mapCoverage": true
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "react-datetime": "^2.10.1",
    "react-toggle": "^4.0.1",
    "react-select": "^1.0.0-rc.10",
    "react-bootstrap": "^0.31.3"
  },
  "peerDependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "bootstrap": "^3.3.7"
  },
  "devDependencies": {
    "react": "^15.5.4",
    "bootstrap": "^3.3.7",
    "react-dom": "^15.5.4",
    "prop-types": "^15.5.10",
    "react-moment-proptypes": "^1.4.0",
    "autoprefixer": "^7.1.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-preset-env": "^1.6.0",
    "babel-preset-jest": "^20.0.3",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "faker": "^4.1.0",
    "file-loader": "^0.11.2",
    "istanbul": "^0.4.5",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.5.3",
    "node-sass": "^4.5.3",
    "postcss-modules-values": "^1.3.0",
    "react-addons-shallow-compare": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "redux-test-utils": "^0.1.2",
    "resolve-url-loader": "^2.1.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.3.0"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:es && npm run build:umd && npm run build:umd:min",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "cross-env NODE_ENV=development webpack src/index.js dist/redux-form-bootstrap-widgets.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack src/index.js dist/redux-form-bootstrap-widgets.js",
    "clean": "rimraf dist es lib",
    "prepublish": " npm run clean && npm run build"
  },
  "npmName": "redux-form-bootstrap-widgets",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ]
}
